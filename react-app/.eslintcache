,"/home/zolvo/CLASS/AUG2020/WEEK21/appron/react-app/src/components/UsersList.js",[],"/home/zolvo/CLASS/AUG2020/WEEK21/appron/react-app/src/components/User.js",[],"/home/zolvo/CLASS/AUG2020/WEEK21/appron/react-app/src/components/Home.js",[],"/home/zolvo/CLASS/AUG2020/WEEK21/appron/react-app/src/components/Main.js",[],"/home/zolvo/CLASS/AUG2020/WEEK21/appron/react-app/src/components/auth/LoginForm.js",[],"/home/zolvo/CLASS/AUG2020/WEEK21/appron/react-app/src/components/auth/ProtectedRoute.js",[],"/home/zolvo/CLASS/AUG2020/WEEK21/appron/react-app/src/services/auth.js",[],"/home/zolvo/CLASS/AUG2020/WEEK21/appron/react-app/src/components/auth/LogoutButton.js",[],"/home/zolvo/CLASS/AUG2020/WEEK21/appron/react-app/src/components/auth/Nopage.js",[],"/home/zolvo/CLASS/AUG2020/WEEK21/appron/react-app/src/components/auth/DemoButton.js",[],"/home/zolvo/CLASS/AUG2020/WEEK21/appron/react-app/src/components/Appointment.js",[],"/home/zolvo/CLASS/AUG2020/WEEK21/appron/react-app/src/components/Chef.js",[],"/home/zolvo/CLASS/AUG2020/WEEK21/appron/react-app/src/components/ChefsList.js",[],"/home/zolvo/CLASS/AUG2020/WEEK21/appron/react-app/src/components/auth/SignUpForm.js",[],"/home/zolvo/CLASS/AUG2020/WEEK21/appron/react-app/src/components/About.js",[],"/home/zolvo/CLASS/AUG2020/WEEK21/appron/react-app/src/components/auth/ChefForm.js",[],"/home/zolvo/CLASS/AUG2020/WEEK21/appron/react-app/src/components/ChefAppointment.js",[],"/home/zolvo/CLASS/AUG2020/WEEK21/appron/react-app/src/components/CommentForm.js",[],"/home/zolvo/CLASS/AUG2020/WEEK21/appron/react-app/src/components/Comments.js",[],"/home/zolvo/CLASS/AUG2020/WEEK21/appron/react-app/src/components/AppointmentForm.js",[],"/home/zolvo/CLASS/SPRINT/appron/react-app/src/index.js",[],["200","201"],"/home/zolvo/CLASS/SPRINT/appron/react-app/src/App.js",[],"/home/zolvo/CLASS/SPRINT/appron/react-app/src/components/NavBar.js",[],"/home/zolvo/CLASS/SPRINT/appron/react-app/src/components/UsersList.js",[],"/home/zolvo/CLASS/SPRINT/appron/react-app/src/components/User.js",["202"],"/home/zolvo/CLASS/SPRINT/appron/react-app/src/components/Home.js",[],"/home/zolvo/CLASS/SPRINT/appron/react-app/src/components/Main.js",[],"/home/zolvo/CLASS/SPRINT/appron/react-app/src/components/ChefsList.js",[],"/home/zolvo/CLASS/SPRINT/appron/react-app/src/components/Chef.js",["203","204"],"import React, { useState, useEffect } from \"react\";\nimport { useHistory, useParams } from \"react-router-dom\";\nimport styled from \"styled-components\";\nimport bg from \"../image/bg.jpg\";\nimport CommentForm from \"./CommentForm\";\nimport Comments from \"./Comments\";\nimport AppointmentForm from \"./AppointmentForm\";\nimport Modal from \"react-modal\";\n\nconst customStyles = {\n  content: {\n    width: \"31.5em\",\n    height: \"60%\",\n    top: \"50%\",\n    left: \"50%\",\n    right: \"auto\",\n    bottom: \"auto\",\n    marginRight: \"-50%\",\n    background: \"rgba(255, 0, 0, 0)\",\n    border:\"none\",\n    transform: \"translate(-50%, -50%)\",\n  },\n};\n\nfunction Chef({ user }) {\n  const [loading, setLoading] = useState(\"\");\n  const [chef, setChef] = useState(\"\");\n  const [showModal, setShowModal] = useState(false);\n  const [showReviews, setShowReviews] = useState(false);\n  const [likes, setLikes] = useState(false);\n  const [count, setCount] = useState(0);\n  // const [rating, setRating] = useState([]);\n  const history = useHistory();\n  const { chefId } = useParams();\n\n  const closeModal = () => {\n    setShowModal(false);\n  };\n\n  // console.log (\"***************\", user)\n  useEffect(() => {\n    document.title = \"Appron: Chef\";\n    if (!chefId) {\n      return;\n    }\n    (async () => {\n      const res = await fetch(`/api/chefs/${chefId}`);\n      const chef = await res.json();\n      setChef(chef);\n    })();\n  }, [chefId]);\n\n  if (!chef) {\n    return null;\n  }\n\n  const toggleReview = () => {\n    if (!user) history.push(\"/login\");\n    setShowReviews(!showReviews);\n  };\n\n  const handleLike = () => {\n    if (!count) {\n      setCount(count + 1);\n    } else {\n      setCount(count - 1);\n    }\n\n    setLikes(!likes);\n  };\n  // console.log(chef)\n  return (\n    <Container>\n      <ChefWrapper className=\"animate__animated animate__fadeInDown\">\n        <Title>Chef Profile</Title>\n        <ul className=\"body\">\n          <li>\n            <strong>Chef Name: </strong> {chef.chef.user.username}\n          </li>\n          <li>\n            <strong>Email:</strong> {chef.chef.user.email}\n          </li>\n          <li>\n            <strong>Address:</strong> {chef.chef.user.address}\n          </li>\n          <li>\n            <strong>City:</strong> {chef.chef.user.city}\n          </li>\n          <li>\n            <strong>State:</strong> {chef.chef.user.state}\n          </li>\n          <li>\n            <strong>ZipCode:</strong> {chef.chef.user.zipcode}\n          </li>\n          <li>\n            <strong>Phone:</strong> {chef.chef.user.phone}\n          </li>\n          <li style={{ marginTop: \"1em\", marginBottom: \"1em\" }}>\n            <strong>About:</strong> {chef.chef.about}\n          </li>\n          <li>\n            <strong>Service:</strong> {chef.chef.service}\n          </li>\n          <li>\n            <strong>Menu:</strong> {chef.chef.menu}\n          </li>\n          <li style={{ marginTop: \"1em\" }}>\n            <strong>Pricing:</strong> {chef.chef.pricing}\n          </li>\n          <li>\n            <strong>Available:</strong> {chef.chef.available ? \"Yes\" : \"No\"}\n          </li>\n        </ul>\n        <button\n          className=\"appointment\"\n          onClick={() => setShowModal(!showModal)}\n        >\n          Make an Appointment\n        </button>\n        <Modal\n          isOpen={showModal}\n          onRequestClose={closeModal}\n          style={customStyles}\n          ariaHideApp={false}\n          contentLabel=\"EXAMPLE MODAL\"\n        >\n          <AppointmentForm user={user} chef={chef} />\n        </Modal>\n        <Goback onClick={history.goBack}>\n          <div>Go Back</div>\n        </Goback>\n        <Box2>\n          <LikeIcon>\n            <span className=\"red\" onClick={handleLike}>\n              {likes ? (\n                <i className=\"fas fa-heart\" />\n              ) : (\n                <i className=\"far fa-heart icon\" />\n              )}\n            </span>\n            <span>{count}</span>\n          </LikeIcon>\n          <ReviewIcon>\n            <i className=\"far fa-comments icon\" onClick={toggleReview} />\n            <span>{chef.comment.length}</span>\n          </ReviewIcon>\n        </Box2>\n        <Box3>{showReviews && <CommentForm user={user} chef={chef} />}</Box3>\n        <Comments user={user} chef={chef} />\n      </ChefWrapper>\n    </Container>\n  );\n}\n\nconst Container = styled.div`\n// background-color: #d81159;\ncolor: grey;\nmin-height: 100vh;\ndisplay:flex;\njustify-content:center;\nbackground-image: url(${bg});\nbackground-repeat: no-repeat;\nbackground-size: center;\nbackground-position: 90% 50%;\n\nli {\n  list-style: none;\n  padding: 0.1em;\n  margin-bottom:3px;\n  font-family: dosis;\n  opacity: 0.9;\n  letter-spacing: 0.06em;\n  font-size: 15px;\n}\n\n.body {\n  color: black;\n  text-shadow: rgba(0, 0, 0, 0.5) 0px 5px 15px;\n}\n\n.appointment{\n  cursor: pointer;\n  box-sizing:border-box;\n  margin-top: 3em;\n  // margin-left: 4em;\n  background-color: #ffbc42;\n  font-weight: 700;\n  width: 18em;\n  height: 3em;\n  border-radius: 2em;\n\n  outline:none;\n  border:none;\n\n  transition: all 0.2x ease-in;\n  &:hover{\n    transform: translateY(-3px);\n  }\n\n\n  `;\n\nconst Goback = styled.button`\n  text-transform: uppercase;\n  margin-top: 0.5em;\n  cursor: pointer;\n  border: none;\n  font-weight: 600;\n  font-size: 15px;\n  font-family: dosis;\n  outline: none;\n\n  transition: all 0.2x ease-in;\n  &:hover {\n    transform: translateY(-3px);\n  }\n`;\n\nconst ChefWrapper = styled.div`\n  